#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules/@unocss/cli/bin/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules/@unocss/cli/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules/@unocss/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules/@unocss/cli/bin/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules/@unocss/cli/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules/@unocss/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/@unocss+cli@66.5.1/node_modules:/home/ubuntu/portfolio-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@unocss+cli@66.5.1/node_modules/@unocss/cli/bin/unocss.mjs" "$@"
else
  exec node  "$basedir/../../../../../@unocss+cli@66.5.1/node_modules/@unocss/cli/bin/unocss.mjs" "$@"
fi
